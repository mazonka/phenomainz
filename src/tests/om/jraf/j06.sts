#!/bin/sh
# test versions: rm save put mv

in=$(basename $0)
in=${in%.*} 

cwd=`pwd`
test -f /usr/bin/cygpath && cwd=`cygpath -m $cwd`

cd $1
./phenod -cqln dwkr=0 s=$cwd/$in.inp jraf_root=jroot
rm -f test.db

echo ""
echo "=========="

prn()
{
echo -n "$1 : "
if test -f $1; then
cat $1 2> /dev/null
else
echo "no"
fi
}

prn jroot/.jraf.ver
prn jroot/aaa/.jraf.ver
prn jroot/aaa/c1.jraf.ver
prn jroot/aaa/c2.jraf.ver
prn jroot/aaa/c3.jraf.ver

#echo ""
#echo "=========="
#ls -1aR jroot
rm -rf jroot

