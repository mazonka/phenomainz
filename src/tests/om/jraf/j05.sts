#!/bin/sh
# test versions

in=$(basename $0)
in=${in%.*} 

cwd=`pwd`
test -f /usr/bin/cygpath && cwd=`cygpath -m $cwd`

cd $1
./phenod -cqln dwkr=0 s=$cwd/$in.inp jraf_root=jroot
rm -f test.db

echo ""
echo "=========="

prn()
{
echo -n "$1 "
cat $1
}

prn jroot/.jraf.ver
prn jroot/aaa/.jraf.ver
prn jroot/aaa/bbb/.jraf.ver
prn jroot/aaa/ccc/.jraf.ver

#echo ""
#echo "=========="
#ls -1aR jroot
rm -rf jroot

